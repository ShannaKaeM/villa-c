{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "color": {
            "palette": [
                {
                    "slug": "primary-light",
                    "color": "#8dabac",
                    "name": "Primary Light"
                },
                {
                    "slug": "primary",
                    "color": "#5a7f80",
                    "name": "Primary"
                },
                {
                    "slug": "primary-dark",
                    "color": "#425a5b",
                    "name": "Primary Dark"
                },
                {
                    "slug": "secondary-light",
                    "color": "#d4dfdb",
                    "name": "Secondary Light"
                },
                {
                    "slug": "secondary",
                    "color": "#bacdc8",
                    "name": "Secondary"
                },
                {
                    "slug": "secondary-dark",
                    "color": "#a2b6b2",
                    "name": "Secondary Dark"
                },
                {
                    "slug": "neutral-light",
                    "color": "#e6e2d7",
                    "name": "Neutral Light"
                },
                {
                    "slug": "neutral",
                    "color": "#cac2ae",
                    "name": "Neutral"
                },
                {
                    "slug": "neutral-dark",
                    "color": "#b6ad9a",
                    "name": "Neutral Dark"
                },
                {
                    "slug": "base-white",
                    "color": "#ffffff",
                    "name": "Base White"
                },
                {
                    "slug": "base-lightest",
                    "color": "#e5e5e5",
                    "name": "Base Lightest"
                },
                {
                    "slug": "base-light",
                    "color": "#bfbfbf",
                    "name": "Base Light"
                },
                {
                    "slug": "base",
                    "color": "#9c9c9c",
                    "name": "Base"
                },
                {
                    "slug": "base-dark",
                    "color": "#737373",
                    "name": "Base Dark"
                },
                {
                    "slug": "base-darkest",
                    "color": "#4d4d4d",
                    "name": "Base Darkest"
                },
                {
                    "slug": "base-black",
                    "color": "#000000",
                    "name": "Base Black"
                }
            ],
            "gradients": [
                {
                    "slug": "primary-to-secondary",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
                    "name": "Primary to Secondary"
                },
                {
                    "slug": "dark-overlay",
                    "gradient": "linear-gradient(180deg, rgba(0,0,0,0.1) 0%, rgba(0,0,0,0.7) 100%)",
                    "name": "Dark Overlay"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#2c3e50",
                        "#ecf0f1"
                    ],
                    "slug": "primary-to-light",
                    "name": "Primary to Light"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "name": "Inter",
                    "fontFamily": "Inter, sans-serif",
                    "slug": "inter"
                },
                {
                    "name": "Georgia",
                    "fontFamily": "Georgia, serif",
                    "slug": "georgia"
                },
                {
                    "name": "Monaco",
                    "fontFamily": "Monaco, monospace",
                    "slug": "monaco"
                }
            ],
            "fontSizes": [
                {
                    "name": "Extra Small",
                    "slug": "xs",
                    "size": "0.75rem"
                },
                {
                    "name": "Small",
                    "slug": "sm",
                    "size": "1rem"
                },
                {
                    "name": "Medium",
                    "slug": "md",
                    "size": "1.125rem"
                },
                {
                    "name": "Large",
                    "slug": "lg",
                    "size": "1.5rem"
                },
                {
                    "name": "Extra Large",
                    "slug": "xl",
                    "size": "2rem"
                },
                {
                    "name": "Extra Extra Large",
                    "slug": "xxl",
                    "size": "3rem"
                },
                {
                    "name": "Extra Extra Extra Large",
                    "slug": "xxxl",
                    "size": "4rem"
                }
            ]
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "%",
                "vh",
                "vw"
            ],
            "spacingSizes": [
                {
                    "name": "10",
                    "slug": "10",
                    "size": "0.5rem"
                },
                {
                    "name": "20",
                    "slug": "20",
                    "size": "1rem"
                },
                {
                    "name": "30",
                    "slug": "30",
                    "size": "1.5rem"
                },
                {
                    "name": "40",
                    "slug": "40",
                    "size": "2rem"
                },
                {
                    "name": "50",
                    "slug": "50",
                    "size": "2.5rem"
                },
                {
                    "name": "60",
                    "slug": "60",
                    "size": "3rem"
                },
                {
                    "name": "70",
                    "slug": "70",
                    "size": "4rem"
                },
                {
                    "name": "80",
                    "slug": "80",
                    "size": "5rem"
                }
            ]
        },
        "layout": {
            "contentSize": "1280px",
            "wideSize": "1450px"
        },
        "custom": {
            "spacing": {
                "small": "max(1.25rem, 5vw)",
                "medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
                "large": "clamp(4rem, 10vw, 8rem)",
                "outer": "var(--wp--custom--spacing--small, 1.25rem)"
            },
            "typography": {
                "font-size": {
                    "heading": "clamp(2.5rem, 5vw, 3.5rem)",
                    "subheading": "clamp(1.5rem, 3vw, 2rem)"
                },
                "line-height": {
                    "tiny": 1.1499999999999999,
                    "small": 1.2,
                    "medium": 1.3999999999999999,
                    "normal": 1.6000000000000001
                }
            },
            "ui": {
                "--ui-card-padding-x": "2rem",
                "--ui-card-padding-y": "1.5rem",
                "--ui-card-radius": "12px",
                "--ui-card-shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
                "--ui-card-spacing": "0.75rem",
                "--ui-button-width": "auto",
                "--ui-button-padding-x": "1rem",
                "--ui-button-padding-y": "0.5rem",
                "--ui-button-radius": "6px",
                "--ui-tag-style1-radius": "20px",
                "--ui-tag-style1-padding-x": "0.75rem",
                "--ui-tag-style1-padding-y": "0.25rem",
                "--ui-tag-style2-radius": "6px",
                "--ui-tag-style2-padding-x": "0.5rem",
                "--ui-tag-style2-padding-y": "0.25rem",
                "--ui-tag-spacing": "0.5rem",
                "--ui-grid-gap": "1.5rem",
                "--ui-content-spacing": "1rem",
                "--ui-section-padding": "4rem"
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--neutral-dark)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--montserrat)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "fontWeight": "400",
            "lineHeight": "1.6"
        },
        "spacing": {
            "blockGap": "1.5rem",
            "padding": {
                "top": "0",
                "right": "var(--wp--preset--spacing--30)",
                "bottom": "0",
                "left": "var(--wp--preset--spacing--30)"
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--secondary)"
                    }
                }
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--montserrat)",
                    "fontWeight": "600",
                    "lineHeight": "1.2"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "button": {
                "border": {
                    "radius": "8px"
                },
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--white)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--secondary)"
                    }
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            }
        },
        "blocks": {
            "core/navigation": {
                "elements": {
                    "link": {
                        ":hover": {
                            "typography": {
                                "textDecoration": "underline"
                            }
                        }
                    }
                }
            }
        }
    }
}